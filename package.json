{
  "name": "etvas-internals",
  "version": "0.1.2",
  "description": "ETVAS Internals",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf es lib",
    "format": "prettier-eslint \"$PWD/src/**/*.{js,jsx}\" --write",
    "lint:js": "eslint 'src/**/*.{js,jsx}'",
    "lint:style": "stylelint 'src/**/*.{css,jsx}'",
    "lint": "npm run lint:js & npm run lint:style",
    "storybook": "BROWSER=none start-storybook -p 6006",
    "build-storybook": "build-storybook -o build/ --quiet",
    "push": "npm run build && git add es && git add lib && git commit -m \"build\" && git push"
  },
  "peerDependencies": {
    "@etvas/etvaskit": "^1",
    "moment": "^2",
    "react": "^16.6.0 || ^17",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@etvas/etvaskit": "^1.1.32",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-viewport": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "babel-loader": "^8.1.0",
    "esbuild": "^0.15.15",
    "esbuild-node-externals": "^1.5.0",
    "esbuild-plugin-browserslist": "^0.5.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "formik": "^2.1.4",
    "lint-staged": "^10.2.7",
    "moment": "^2",
    "prettier-eslint": "^9.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "storybook": "^6.3.7",
    "styled-components": "^5.1.1",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "author": "Etvas",
  "license": "MIT",
  "keywords": [
    "etvas"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "highcharts": "^9.1.2",
    "highcharts-react-official": "^3.0.0",
    "yup": "^1.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
