{
  "name": "etvas-internals",
  "version": "0.1.1",
  "description": "ETVAS Internals",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "prebuild:link": "mv node_modules_ node_modules",
    "build:link": "npm run build -- --no-demo",
    "postbuild:link": "mv node_modules node_modules_",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build -- --copy-files",
    "start": "BROWSER=none nwb serve-react-demo",
    "format": "prettier-eslint \"$PWD/src/**/*.{js,jsx}\" --write",
    "lint:js": "eslint 'src/**/*.{js,jsx}'",
    "lint:style": "stylelint 'src/**/*.{css,jsx}'",
    "lint": "npm run lint:js & npm run lint:style",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "storybook": "BROWSER=none start-storybook -p 6006",
    "build-storybook": "build-storybook -o build/ --quiet",
    "push": "npm run build && git add es && git add lib && git commit -m \"build\" && gti push"
  },
  "peerDependencies": {
    "react": "^16.6.0 || ^17",
    "styled-components": "^5.1.0",
    "@etvas/etvaskit": "^1",
    "moment": "^2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@etvas/etvaskit": "^1",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-viewport": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "formik": "^2.1.4",
    "lint-staged": "^10.2.7",
    "moment": "^2",
    "nwb": "^0.25.2",
    "prettier-eslint": "^9.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "storybook": "^6.3.7",
    "styled-components": "^5.1.1",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "author": "Etvas",
  "license": "MIT",
  "keywords": [
    "etvas"
  ],
  "dependencies": {}
}
